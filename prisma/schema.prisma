generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model Message {
  id      String   @id @default(uuid())
  from    String
  to      String
  subject String
  date    DateTime
}

model MessageWatermark {
  id   Int      @id @default(autoincrement())
  date DateTime
}
